<view bind:__l="__l"><view class="cu-bar bg-white solid-bottom" style="height:200rpx;" id="top-box"><view class="header"><view><text style="font-size:36rpx;">测试考试</text><text style="font-size:.28upx;">{{this.subjectIndex+1+"/"+subjectList.length}}</text></view><view><text style="font-size:24rpx;">倒计时</text><text style="font-size:36rpx;">99:99</text></view></view></view><form><view style="background:#f1f1f1;height:0.3rem;"></view><swiper class="swiper-box" style="{{'height:'+(swiperHeight)+';'}}" current="{{subjectIndex}}" data-event-opts="{{[['change',[['SwiperChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{$root.l1}}" wx:for-item="subject" wx:for-index="index"><swiper-item><view class="action text-black" style="position:absolute;top:50rpx;color:#d1d1d1;left:50rpx;font-size:24rpx;"><block wx:if="{{currentType===1}}"><text>判断</text></block><block wx:else><block wx:if="{{currentType===2}}"><text>单选</text></block><block wx:else><block wx:if="{{currentType===3}}"><text>多选</text></block><block wx:else><block wx:if="{{currentType===4}}"><text>填空</text></block><block wx:else><block wx:if="{{currentType===5}}"><text>问答</text></block></block></block></block></block></view><block wx:if="{{index-subjectIndex>=-1&&index-subjectIndex<=1}}"><view style="margin-left:15%;margin-top:50rpx;"><view class="cu-bar bg-white solid-bottom"><view class="action text-black">{{'第'+(subjectIndex+1)+"题:"+subject.$orig.title+''}}</view></view><view><block wx:if="{{subject.$orig.type===1||subject.$orig.type===2}}"><radio-group data-event-opts="{{[['change',[['RadioboxChange',['$event']]]]]}}" class="block" bindchange="__e"><block wx:for="{{subject.$orig.optionList}}" wx:for-item="option" wx:for-index="__i0__"><view class="cu-form-group"><radio value="{{option.id}}"></radio><view class="title text-black">{{option.id+"."+option.content}}</view></view></block></radio-group></block><block wx:else><block wx:if="{{subject.$orig.type===3}}"><checkbox-group data-event-opts="{{[['change',[['CheckboxChange',['$event']]]]]}}" class="block" bindchange="__e"><block wx:for="{{subject.l0}}" wx:for-item="option" wx:for-index="__i1__"><view class="cu-form-group"><checkbox class="{{[option.g0>-1?'checked':'']}}" value="{{option.$orig.id}}" checked="{{option.g1>-1?true:false}}"></checkbox><view class="title  text-black">{{option.$orig.id+"."+option.$orig.content}}</view></view></block></checkbox-group></block><block wx:else><block wx:if="{{subject.$orig.type===4}}"><view><view class="cu-form-group solid-bottom"><view class="title  text-black">答：</view><input placeholder="文本输入框" name="input" data-event-opts="{{[['blur',[['textInput',['$event']]]]]}}" bindblur="__e"/></view></view></block><block wx:else><block wx:if="{{subject.$orig.type===5}}"><view><view class="cu-bar cu-bar-title bg-white margin-top"><view class="action  text-black">答：</view></view><view class="cu-form-group solid margin"><textarea maxlength="-1" placeholder="多行文本输入框" data-event-opts="{{[['blur',[['textInput',['$event']]]]]}}" bindblur="__e"></textarea></view></view></block></block></block></block></view><view hidden="{{!(subject.$orig.showAnswer)}}" class="margin-top solid-top"><view class="cu-bar"><view class="action  text-grey"><text>正确答案：</text><text class="solid-bottom  padding-left text-green">{{subject.$orig.answer}}</text></view></view><view class="cu-bar cu-bar-title"><view class="action  text-grey"><text>解析：</text></view></view><view class="text-content padding  text-grey">{{''+subject.$orig.explain+''}}</view></view></view></block></swiper-item></block></swiper></form><view class="cu-bar tabbar bg-white shadow foot" id="foot-box"><view data-event-opts="{{[['tap',[['MoveSubject',[-1]]]]]}}" class="action" bindtap="__e"><view class="text-gray">上一题</view></view><view data-event-opts="{{[['tap',[['MoveSubject',[1]]]]]}}" class="action" style="color:white;background:#FF8864;border:none;" bindtap="__e"><view class="text-gray" style="color:white;">{{submitnext}}</view></view></view></view>